{
  "name": "naming-suggestor-extension",
  "displayName": "Naming Suggestor",
  "description": "AI-powered variable and function naming suggestions using OpenAI",
  "version": "1.0.0",
  "publisher": "bini",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Formatters"
  ],
  "keywords": [
    "ai",
    "openai",
    "naming",
    "refactoring",
    "variables",
    "functions",
    "code quality"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/naming-suggestor-extension"
  },
  "bugs": {
    "url": "https://github.com/your-username/naming-suggestor-extension/issues"
  },
  "homepage": "https://github.com/your-username/naming-suggestor-extension#readme",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "namingSuggestor.suggestNames",
        "title": "Suggest Better Names",
        "category": "Naming Suggestor"
      }
    ],
    "keybindings": [
      {
        "command": "namingSuggestor.suggestNames",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus && editorHasSelection"
      }
    ],
    "configuration": {
      "title": "Naming Suggestor",
      "properties": {
        "namingSuggestor.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key for generating naming suggestions",
          "markdownDescription": "Your OpenAI API key. You can get one from [OpenAI's website](https://platform.openai.com/api-keys)"
        },
        "namingSuggestor.model": {
          "type": "string",
          "default": "gpt-3.5-turbo",
          "enum": [
            "gpt-3.5-turbo",
            "gpt-4",
            "gpt-4-turbo"
          ],
          "description": "OpenAI model to use for suggestions"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "openai": "^4.0.0"
  },
  "packageManager": "pnpm@9.12.0+sha256.a61b67ff6cc97af864564f4442556c22a04f2e5a7714fbee76a1011361d9b726"
}